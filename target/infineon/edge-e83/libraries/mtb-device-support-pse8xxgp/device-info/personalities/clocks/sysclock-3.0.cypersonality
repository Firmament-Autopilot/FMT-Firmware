      <Line value='SystemCoreClockSetup((CY_SYSCLK_CLK_CORE_HF_PATH_NUM == 0 ? clk_hf0_freq_hz : clk_hf1_freq_hz), clk_hf0_freq_hz);' />
      <Line value='' />
      <Line value='#endif /* (!defined(COMPONENT_SECURE_DEVICE)) */' />
    </ConfigFunction>

