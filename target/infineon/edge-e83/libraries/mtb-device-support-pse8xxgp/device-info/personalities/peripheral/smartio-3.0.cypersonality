        <Entry name='I/O 1' value='CY_SMARTIO_LUTTR_IO1' visible='`${hasAnyIo$idx ? (($idx lt 4) &amp;&amp; (clkSrc ne CY_SMARTIO_CLK_IO1)) : false}`' />
