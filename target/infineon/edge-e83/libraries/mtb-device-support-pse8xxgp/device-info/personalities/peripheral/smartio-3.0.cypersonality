      <Entry name='LUT1' value='CY_SMARTIO_DUTR_LUT1_OUT' visible='true' />
      <Entry name='LUT2' value='CY_SMARTIO_DUTR_LUT2_OUT' visible='true' />
      <Entry name='LUT3' value='CY_SMARTIO_DUTR_LUT3_OUT' visible='true' />
