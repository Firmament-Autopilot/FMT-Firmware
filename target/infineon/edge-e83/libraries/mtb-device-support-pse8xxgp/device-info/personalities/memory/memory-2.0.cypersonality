        <FixIt action='SET_PARAM' target='`${memoryRegionLocation(33)}`.memoryId' value='`${memoryId15}`' valid='`${15 &lt; numMemories &amp;&amp; regionPhysMemId_33 eq invalidUsedMemoryId$idx{mem}}`' />
        <FixIt action='SET_PARAM' target='`${memoryRegionLocation(34)}`.memoryId' value='`${memoryId15}`' valid='`${15 &lt; numMemories &amp;&amp; regionPhysMemId_34 eq invalidUsedMemoryId$idx{mem}}`' />
        <FixIt action='SET_PARAM' target='`${memoryRegionLocation(35)}`.memoryId' value='`${memoryId15}`' valid='`${15 &lt; numMemories &amp;&amp; regionPhysMemId_35 eq invalidUsedMemoryId$idx{mem}}`' />
        <FixIt action='SET_PARAM' target='`${memoryRegionLocation(36)}`.memoryId' value='`${memoryId15}`' valid='`${15 &lt; numMemories &amp;&amp; regionPhysMemId_36 eq invalidUsedMemoryId$idx{mem}}`' />
        <FixIt action='SET_PARAM' target='`${memoryRegionLocation(37)}`.memoryId' value='`${memoryId15}`' valid='`${15 &lt; numMemories &amp;&amp; regionPhysMemId_37 eq invalidUsedMemoryId$idx{mem}}`' />
        <FixIt action='SET_PARAM' target='`${memoryRegionLocation(38)}`.memoryId' value='`${memoryId15}`' valid='`${15 &lt; numMemories &amp;&amp; regionPhysMemId_38 eq invalidUsedMemoryId$idx{mem}}`' />
        <FixIt action='SET_PARAM' target='`${memoryRegionLocation(39)}`.memoryId' value='`${memoryId15}`' valid='`${15 &lt; numMemories &amp;&amp; regionPhysMemId_39 eq invalidUsedMemoryId$idx{mem}}`' />
        <FixIt action='SET_PARAM' target='`${memoryRegionLocation(40)}`.memoryId' value='`${memoryId15}`' valid='`${15 &lt; numMemories &amp;&amp; regionPhysMemId_40 eq invalidUsedMemoryId$idx{mem}}`' />
        <FixIt action='SET_PARAM' target='`${memoryRegionLocation(41)}`.memoryId' value='`${memoryId15}`' valid='`${15 &lt; numMemories &amp;&amp; regionPhysMemId_41 eq invalidUsedMemoryId$idx{mem}}`' />
        <FixIt action='SET_PARAM' target='`${memoryRegionLocation(42)}`.memoryId' value='`${memoryId15}`' valid='`${15 &lt; numMemories &amp;&amp; regionPhysMemId_42 eq invalidUsedMemoryId$idx{mem}}`' />
        <FixIt action='SET_PARAM' target='`${memoryRegionLocation(43)}`.memoryId' value='`${memoryId15}`' valid='`${15 &lt; numMemories &amp;&amp; regionPhysMemId_43 eq invalidUsedMemoryId$idx{mem}}`' />
        <FixIt action='SET_PARAM' target='`${memoryRegionLocation(44)}`.memoryId' value='`${memoryId15}`' valid='`${15 &lt; numMemories &amp;&amp; regionPhysMemId_44 eq invalidUsedMemoryId$idx{mem}}`' />
        <FixIt action='SET_PARAM' target='`${memoryRegionLocation(45)}`.memoryId' value='`${memoryId15}`' valid='`${15 &lt; numMemories &amp;&amp; regionPhysMemId_45 eq invalidUsedMemoryId$idx{mem}}`' />
        <FixIt action='SET_PARAM' target='`${memoryRegionLocation(46)}`.memoryId' value='`${memoryId15}`' valid='`${15 &lt; numMemories &amp;&amp; regionPhysMemId_46 eq invalidUsedMemoryId$idx{mem}}`' />
        <FixIt action='SET_PARAM' target='`${memoryRegionLocation(47)}`.memoryId' value='`${memoryId15}`' valid='`${15 &lt; numMemories &amp;&amp; regionPhysMemId_47 eq invalidUsedMemoryId$idx{mem}}`' />
        <FixIt action='SET_PARAM' target='`${memoryRegionLocation(48)}`.memoryId' value='`${memoryId15}`' valid='`${15 &lt; numMemories &amp;&amp; regionPhysMemId_48 eq invalidUsedMemoryId$idx{mem}}`' />
        <FixIt action='SET_PARAM' target='`${memoryRegionLocation(49)}`.memoryId' value='`${memoryId15}`' valid='`${15 &lt; numMemories &amp;&amp; regionPhysMemId_49 eq invalidUsedMemoryId$idx{mem}}`' />
        <FixIt action='SET_PARAM' target='`${memoryRegionLocation(50)}`.memoryId' value='`${memoryId15}`' valid='`${15 &lt; numMemories &amp;&amp; regionPhysMemId_50 eq invalidUsedMemoryId$idx{mem}}`' />
        <FixIt action='SET_PARAM' target='`${memoryRegionLocation(51)}`.memoryId' value='`${memoryId15}`' valid='`${15 &lt; numMemories &amp;&amp; regionPhysMemId_51 eq invalidUsedMemoryId$idx{mem}}`' />
        <FixIt action='SET_PARAM' target='`${memoryRegionLocation(52)}`.memoryId' value='`${memoryId15}`' valid='`${15 &lt; numMemories &amp;&amp; regionPhysMemId_52 eq invalidUsedMemoryId$idx{mem}}`' />
        <FixIt action='SET_PARAM' target='`${memoryRegionLocation(53)}`.memoryId' value='`${memoryId15}`' valid='`${15 &lt; numMemories &amp;&amp; regionPhysMemId_53 eq invalidUsedMemoryId$idx{mem}}`' />
        <FixIt action='SET_PARAM' target='`${memoryRegionLocation(54)}`.memoryId' value='`${memoryId15}`' valid='`${15 &lt; numMemories &amp;&amp; regionPhysMemId_54 eq invalidUsedMemoryId$idx{mem}}`' />
        <FixIt action='SET_PARAM' target='`${memoryRegionLocation(55)}`.memoryId' value='`${memoryId15}`' valid='`${15 &lt; numMemories &amp;&amp; regionPhysMemId_55 eq invalidUsedMemoryId$idx{mem}}`' />
        <FixIt action='SET_PARAM' target='`${memoryRegionLocation(56)}`.memoryId' value='`${memoryId15}`' valid='`${15 &lt; numMemories &amp;&amp; regionPhysMemId_56 eq invalidUsedMemoryId$idx{mem}}`' />
        <FixIt action='SET_PARAM' target='`${memoryRegionLocation(57)}`.memoryId' value='`${memoryId15}`' valid='`${15 &lt; numMemories &amp;&amp; regionPhysMemId_57 eq invalidUsedMemoryId$idx{mem}}`' />
        <FixIt action='SET_PARAM' target='`${memoryRegionLocation(58)}`.memoryId' value='`${memoryId15}`' valid='`${15 &lt; numMemories &amp;&amp; regionPhysMemId_58 eq invalidUsedMemoryId$idx{mem}}`' />
        <FixIt action='SET_PARAM' target='`${memoryRegionLocation(59)}`.memoryId' value='`${memoryId15}`' valid='`${15 &lt; numMemories &amp;&amp; regionPhysMemId_59 eq invalidUsedMemoryId$idx{mem}}`' />
        <FixIt action='SET_PARAM' target='`${memoryRegionLocation(60)}`.memoryId' value='`${memoryId15}`' valid='`${15 &lt; numMemories &amp;&amp; regionPhysMemId_60 eq invalidUsedMemoryId$idx{mem}}`' />
        <FixIt action='SET_PARAM' target='`${memoryRegionLocation(61)}`.memoryId' value='`${memoryId15}`' valid='`${15 &lt; numMemories &amp;&amp; regionPhysMemId_61 eq invalidUsedMemoryId$idx{mem}}`' />
        <FixIt action='SET_PARAM' target='`${memoryRegionLocation(62)}`.memoryId' value='`${memoryId15}`' valid='`${15 &lt; numMemories &amp;&amp; regionPhysMemId_62 eq invalidUsedMemoryId$idx{mem}}`' />
        <FixIt action='SET_PARAM' target='`${memoryRegionLocation(63)}`.memoryId' value='`${memoryId15}`' valid='`${15 &lt; numMemories &amp;&amp; regionPhysMemId_63 eq invalidUsedMemoryId$idx{mem}}`' />
      </FixItBatch>
    </DRC>
    <!-- regionOffset_$idx must be within memorySize -->
    <DRC condition='`${$idx &lt; numRegionsDefined &amp;&amp; hasMemory(regionPhysMemId_$idx) &amp;&amp; regionOffset_$idx &gt;= memorySize(regionPhysMemId_$idx)}`' type='ERROR'
         text='Memory region "`${regionId_$idx}`" must begin within physical memory "`${memoryDisplayName(regionPhysMemId_$idx)}`".'
         repeatCount='$MAX_REGIONS' paramId='$idx' />
    <!-- regionOffset_$idx is unsigned so it cannot be less than 0 -->
    <DRC condition='`${$idx &lt; numRegionsDefined &amp;&amp; hasMemory(regionPhysMemId_$idx) &amp;&amp; regionOffset_$idx &lt; memorySize(regionPhysMemId_$idx) &amp;&amp; regionEndOffset_$idx &gt; memorySize(regionPhysMemId_$idx)}`' type='ERROR'
         text='Memory region "`${regionId_$idx}`" does not fit within physical memory "`${memoryDisplayName(regionPhysMemId_$idx)}`".'
         repeatCount='$MAX_REGIONS' paramId='$idx' >
      <FixIt action='SET_PARAM' target='`${memoryRegionLocation($idx)}`.size' value='`${toHex(memorySize(regionPhysMemId_$idx) - regionOffset_$idx)}`' valid='`${(memorySize(regionPhysMemId_$idx) - regionOffset_$idx) &gt; 0}`' description='Change size of memory region "`${regionId_$idx}`" to `${toHex(memorySize(regionPhysMemId_$idx) - regionOffset_$idx)}`' />
    </DRC>
    <!-- Regions must not overlap -->
    <DRC condition='`${($idx+1) &lt; numRegionsDefined &amp;&amp; regionPhysMemId_$idx eq lookupExpression("regionPhysMemId_" . ($idx+1)) &amp;&amp; regionEndOffset_$idx &gt; lookupExpression("regionOffset_" . ($idx+1)) }`'
         type='ERROR' repeatCount='255'
         text='Memory region "`${regionId_$idx}`" overlaps with memory region "`${lookupExpression("regionId_" . ($idx+1))}`". Note: The &apos;Autocorrect overlaps&apos; feature on the Memory tab can be used to fix this issue.' paramId='$idx' >
      <!-- Set size of overlapping region to as large as it can be without overlapping the next region and while still being aligned with the physical memory constraints -->
      <FixIt action='SET_PARAM' target='`${memoryRegionLocation($idx)}`.size' value='`${toHex(floor((lookupExpression("regionOffset_" . ($idx+1)) - regionOffset_$idx)/memoryAlignment(regionPhysMemId_$idx))*memoryAlignment(regionPhysMemId_$idx))}`' valid='`${floor((lookupExpression("regionOffset_" . ($idx+1)) - regionOffset_$idx)/memoryAlignment(regionPhysMemId_$idx))*memoryAlignment(regionPhysMemId_$idx) &gt; 0}`' description='Change size of memory region "`${regionId_$idx}`" to `${toHex(floor((lookupExpression("regionOffset_" . ($idx+1)) - regionOffset_$idx)/memoryAlignment(regionPhysMemId_$idx))*memoryAlignment(regionPhysMemId_$idx))}`' />
    </DRC>
    <!-- Regions that have different domains should not share a memory block -->
    <DRC condition='`${($idx+1) &lt; numRegionsDefined &amp;&amp; regionPhysMemId_$idx eq lookupExpression("regionPhysMemId_" . ($idx+1)) &amp;&amp; regionEndOffset_$idx &lt;= lookupExpression("regionOffset_" . ($idx+1)) &amp;&amp; (regionEndOffset_$idx-1)/memoryAlignment(regionPhysMemId_$idx) == lookupExpression("regionOffset_" . ($idx+1))/memoryAlignment(regionPhysMemId_$idx) &amp;&amp; regionDomain_$idx ne lookupExpression("regionDomain_" . ($idx+1)) }`'
         type='INFO' repeatCount='255'
         text='Memory regions "`${regionId_$idx}`" and "`${lookupExpression("regionId_" . ($idx+1))}`" share the same erase block (size `${toHex(memoryAlignment(regionPhysMemId_$idx))}`) but are assigned to different domains. Reprogramming one domain will erase the data used by the other domain.' paramId='$idx' />
  </DRCs>

  <ConfigFirmware>
    <!-- cymem_memory_types.h -->
    <ConfigFile repeatCount='1' name='cymem_memory_types.h'>
      <Line value='/* This file is automatically generated. */'/>
      <Line value='#if !defined(CYMEM_MEMORY_TYPES_H)' />
      <Line value='#define CYMEM_MEMORY_TYPES_H'/>
      <Line value=''/>
      <Repeat count='`${numMemories}`' repeatId='mem'>
        <Line value='#define $MEMORY_LOCATION_PREFIX`${memoryId$idx{mem}}` ($idx{mem})' />
      </Repeat>
      <Line value=''/>
      <Line value='#define IS_SMIF_MEMORY(mem) ( \'/>
      <Repeat count='`${numMemories}`' repeatId='mem'>
        <Line value='    (mem == $MEMORY_LOCATION_PREFIX`${memoryId$idx{mem}}`) ? 1 : \' include='`${hasMatch(memoryId$idx{mem}, "^SMIF")}`' />
      </Repeat>
      <Line value='    0 \'/>
      <Line value=')'/>
      <Line value=''/>
      <Line value='#endif /* CYMEM_MEMORY_TYPES_H */'/>
    </ConfigFile>

    <!-- cymem_memory_locations.h -->
