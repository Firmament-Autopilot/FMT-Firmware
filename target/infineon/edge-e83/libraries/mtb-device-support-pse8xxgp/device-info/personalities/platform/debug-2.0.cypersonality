    <ConfigInstruction value='Cy_SysClk_PeriphAssignDivider(`${pclkDst}`, `${getExposedMember(sourceClock, "clockSel")}`);' include='`${(hasMatch(getIpBlockName(), "m4cpuss.*") &amp;&amp; pclkOk)}`'  />
  </ConfigFirmware>
