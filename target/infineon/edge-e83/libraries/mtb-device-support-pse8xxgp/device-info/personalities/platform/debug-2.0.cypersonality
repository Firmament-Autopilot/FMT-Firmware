    <ParamSignal port='swj_trstn[0]' name='TRSTn' group='`${dbgGroup}`' visible='`${(hasVisibleOption("swj_trstn[0]")) &amp;&amp; (dbgMode eq JTAG)}`' desc='Reserve pin for Test Reset' canBeEmpty='true'>
      <Constraint type='ACCEPT' targetLocation='ioss\[\d+\]\.port\[\d+\]\.pin.*' valid='true' >
