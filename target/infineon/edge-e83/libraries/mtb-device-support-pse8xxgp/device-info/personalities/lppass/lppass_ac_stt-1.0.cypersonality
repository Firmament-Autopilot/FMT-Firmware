          editable='`${((action == 3) || (action == 4) || (action == 5) || (action == 6))}`'>
        <Entry name='State $idx' value='$idx'
               visible='`${(isBlockUsed(resource_state_$idx) &amp;&amp; ($idx ne getInstNumber("state"))) || ((0 eq getInstNumber("state")) &amp;&amp; !(isBlockUsed(resource_state_1)) &amp;&amp; ($idx eq 0) &amp;&amp; !((action == 3) || (action == 4) || (action == 5) || (action == 6)))}`'
               repeatCount='64'/>
      </ParamChoice>
      <ParamRange id='writeCounter'
                  name='Write Counter'
                  min='1'
                  max='4096'
                  resolution='1'
