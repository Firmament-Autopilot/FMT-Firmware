    <DRC type='ERROR'
         text='The minimum gain for Opamp $idx in CTB 1 is not aligned with gain from the State Transition Table.'
         repeatCount='2'
         condition='`${ctb1opamp$idxEnable &amp;&amp; isBlockUsed(resource_pass . ".ctb[1].oa[$idx]") &amp;&amp;
