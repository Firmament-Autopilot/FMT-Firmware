                      default='0'>
            <Entry name='0' value='0' visible='`${isBlockUsed("pass[" . inst_pass . "].ctb[$idx{1}].oa[$idx{0}].ctb_dynamic[0]")}`'/>
            <Entry name='1' value='1' visible='`${isBlockUsed("pass[" . inst_pass . "].ctb[$idx{1}].oa[$idx{0}].ctb_dynamic[1]")}`'/>
            <Entry name='2' value='2' visible='`${isBlockUsed("pass[" . inst_pass . "].ctb[$idx{1}].oa[$idx{0}].ctb_dynamic[2]")}`'/>
            <Entry name='3' value='3' visible='`${isBlockUsed("pass[" . inst_pass . "].ctb[$idx{1}].oa[$idx{0}].ctb_dynamic[3]")}`'/>
            <Entry name='4' value='4' visible='`${isBlockUsed("pass[" . inst_pass . "].ctb[$idx{1}].oa[$idx{0}].ctb_dynamic[4]")}`'/>
            <Entry name='5' value='5' visible='`${isBlockUsed("pass[" . inst_pass . "].ctb[$idx{1}].oa[$idx{0}].ctb_dynamic[5]")}`'/>
            <Entry name='6' value='6' visible='`${isBlockUsed("pass[" . inst_pass . "].ctb[$idx{1}].oa[$idx{0}].ctb_dynamic[6]")}`'/>
            <Entry name='7' value='7' visible='`${isBlockUsed("pass[" . inst_pass . "].ctb[$idx{1}].oa[$idx{0}].ctb_dynamic[7]")}`'/>
          </ParamChoice>

          <ParamString id='ctb$idx{1}opamp$idx{0}DynCfgWrapper'
                        name='ctb$idx{1}opamp$idx{0}DynCfgWrapper'
                        group='Internal'
                        editable='false'
                        visible='false'
                        desc=''
                        default='`${(ctb$idx{1}opamp$idx{0}Enable &amp;&amp; ctb$idx{1}OpAmp$idx{0}IsUsed &amp;&amp; getExposedMember("pass[" .inst_pass . "].ctb[$idx{1}]", "ctb_oa$idx{0}_dynCfgCnt") > 0 ) ? ctb$idx{1}opamp$idx{0}DynCfg : -1}`'/>

          <ParamChoice id='ctb$idx{1}opamp$idx{0}Gain'
                      name='Opamp $idx{0} Gain'
                      group='CTB $idx{1}'
                      visible='`${ctb$idx{1}opamp$idx{0}Enable}`'
                      desc='Defines the gain used for Opamp $idx{0}, not used for buffer, opamp with open loop and comparator.'
                      editable='`${ctb$idx{1}opamp$idx{0}Enable &amp;&amp; ctb$idx{1}OpAmp$idx{0}IsUsed}`'
