                   default='false'/>

        <ParamChoice id='stateComp$idxDynCfg'
                     name='Comparator $idx Dynamic Config'
                     group='PTComp'
                     visible='`${stateUnlockPtComp &amp;&amp; stateComp$idxEnable}`'
                     editable='`${ptcStaticComp$idxPower ne "CY_AUTANALOG_PTCOMP_COMP_PWR_OFF"}`'
                     desc='Selects the dynamic configuration used for PTComp Comparator $idx'
                     default='0'>
          <Entry name='0' value='0' visible='`${isBlockUsed(resource_ptc . ".ptc_dynamic[0]")}`'/>
          <Entry name='1' value='1' visible='`${isBlockUsed(resource_ptc . ".ptc_dynamic[1]")}`'/>
          <Entry name='2' value='2' visible='`${isBlockUsed(resource_ptc . ".ptc_dynamic[2]")}`'/>
          <Entry name='3' value='3' visible='`${isBlockUsed(resource_ptc . ".ptc_dynamic[3]")}`'/>
          <Entry name='4' value='4' visible='`${isBlockUsed(resource_ptc . ".ptc_dynamic[4]")}`'/>
          <Entry name='5' value='5' visible='`${isBlockUsed(resource_ptc . ".ptc_dynamic[5]")}`'/>
          <Entry name='6' value='6' visible='`${isBlockUsed(resource_ptc . ".ptc_dynamic[6]")}`'/>
          <Entry name='7' value='7' visible='`${isBlockUsed(resource_ptc . ".ptc_dynamic[7]")}`'/>
        </ParamChoice>

        <ParamString id='stateComp$idxDynCfgWrapper'
                      name='stateComp$idxDynCfgWrapper'
                      group='PTComp'
                      editable='false'
                      visible='false'
                      desc=''
                      default='`${(ptcStaticComp$idxPower ne "CY_AUTANALOG_PTCOMP_COMP_PWR_OFF" &amp;&amp; getExposedMember(resource_ptc, "ptc_sta_dynCfgCnt") > 0)  ? stateComp$idxDynCfg : -1}`'/>
      </Repeat>

<!-- STT ADC begin -->
      <ParamBool id='sarAdcUnlock'
                 name='Unlock SAR ADC'
                 group='SAR ADC'
                 editable='`${hasBlock("pass[0].sar[0]") &amp;&amp; isBlockUsed(resource_sar) ? (getExposedMember(resource_sar, "sar_adcIsUsed")) : false}`'
                 visible='`${hasBlock("pass[0].sar[0]")}`'
                 desc='Enable update of ADC fields in State Transition Table'
                 default='false'/>

      <ParamBool id='sarAdcEnable'
                 name='SAR ADC Enabled'
                 group='SAR ADC'
                 editable='`${(sarAdcUnlock)}`'
                 visible='`${(sarAdcUnlock)}`'
                 desc='Allows ADC control from the Autonomous Controller'
                 default='false'/>

      <ParamBool id='sarAdcTrigger'
                 name='SAR ADC Trigger'
                 group='SAR ADC'
                 editable='`${(sarAdcEnable)}`'
                 visible='`${(sarAdcEnable)}`'
                 desc='Initiates ADC operation for the selected channels'
                 default='false'/>

    <ParamChoice id='sarAdcScanGroup'
                 name='SAR ADC Scan Group'
                 group='SAR ADC'
                 visible='`${(sarAdcTrigger)}`'
                 editable='`${(sarAdcTrigger)}`'
                 desc='Select the group of channels to convert'
                 default='0'>
      <Entry name='Group $idx' value='$idx' visible='`${isBlockUsed("pass[" . inst_pass . "].sar[0].group[$idx]")}`' repeatCount='32'/>
    </ParamChoice>

    <ParamString id='sarAdcScanGroupWrapper'
                 name='sarAdcScanGroupWrapper'
                  group='SAR ADC'
                  editable='false'
                  visible='false'
                  desc=''
                  default='`${sarAdcTrigger &amp;&amp; getExposedMember(resource_sar, "sar_scanCnt") > 0 ? sarAdcScanGroup : -1}`'/>

