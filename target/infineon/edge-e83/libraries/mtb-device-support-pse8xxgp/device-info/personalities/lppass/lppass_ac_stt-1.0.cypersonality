      </Repeat>

      <ParamString id='ptcIsUsed' name='ptcIsUsed' group='Internal' editable='false' visible='false' desc='' default='`${isBlockUsed(resource_ptc)}`' />
      <Repeat count='2'>
        <ParamString id='ptcStaticComp$idxPower' name='ptcStaticComp$idxPower' group='Internal' editable='false' visible='false' desc=''
                     default='`${ptcIsUsed ? getExposedMember(resource_ptc, "ptc_sta_comp$idxPower") : "CY_AUTANALOG_PTCOMP_COMP_PWR_OFF"}`' />
      </Repeat>

      <ParamBool id='stateUnlockPtComp'
                 name='Unlock PTComp'
                 group='PTComp'
                 editable='`${hasBlock("pass[0].ptc[0]") &amp;&amp; ptcIsUsed &amp;&amp; (ptcStaticComp0Power ne "CY_AUTANALOG_PTCOMP_COMP_PWR_OFF" || ptcStaticComp1Power ne "CY_AUTANALOG_PTCOMP_COMP_PWR_OFF")}`'
                 visible='`${hasBlock("pass[0].ptc[0]")}`'
                 desc='Unlocks the PTComp fields'
                 default='false'/>

      <Repeat count='2'>
        <ParamBool id='stateComp$idxEnable'
                   name='Comparator $idx Enable'
                   group='PTComp'
                   visible='`${stateUnlockPtComp}`'
                   editable='`${ptcStaticComp$idxPower ne "CY_AUTANALOG_PTCOMP_COMP_PWR_OFF"}`'
