      </ParamChoice>

      <ParamChoice
          group='State Control'
          id='conditionPtcomp'
          name='Condition'
          desc='Condition for PTCOMP'
          visible='`${(conditionSrc == 3) &amp;&amp; conditionSrcVisible}`'
