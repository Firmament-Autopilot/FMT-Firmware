      <ParamString id='wakeupSrcCTB1Comp0Desc' name='' group='State Control' desc='' visible='`${ (action eq 7) &amp;&amp; (wakeupSrc eq "CY_AUTANALOG_STT_AC_CONDITION_CTB1_CMP0_WAKE") }`'  editable='false'
                   default='CTB 1 Comparator 0 Edge Detect Condition'/>
      <ParamString id='wakeupSrcCTB1Comp1Desc' name='' group='State Control' desc='' visible='`${ (action eq 7) &amp;&amp; (wakeupSrc eq "CY_AUTANALOG_STT_AC_CONDITION_CTB1_CMP1_WAKE") }`'  editable='false'
                   default='CTB 1 Comparator 1 Edge Detect Condition'/>

      <ParamChoice
          group='State Control'
          id='conditionSttInt'
          name='Condition'
          desc='Condition for AC internal sources'
          visible='`${(conditionSrc == 0) &amp;&amp; conditionSrcVisible}`'
          default='CY_AUTANALOG_STT_AC_CONDITION_CNT_DONE'
          editable='`${(conditionSrc == 0) &amp;&amp; conditionSrcVisible}`'>
        <Entry name='True'                         value='CY_AUTANALOG_STT_AC_CONDITION_TRUE'               visible='`${(action == 3) || (action == 5)}`'/>
        <Entry name='Enabled Blocks Ready'         value='CY_AUTANALOG_STT_AC_CONDITION_BLOCK_READY'        visible='true'/>
        <Entry name='Counter Done'                 value='CY_AUTANALOG_STT_AC_CONDITION_CNT_DONE'           visible='true'/>
      </ParamChoice>

      <ParamChoice
          group='State Control'
          id='conditionSttExt'
          name='Condition'
          desc='Condition for AC external/CPU sources'
          visible='`${(conditionSrc == 1) &amp;&amp; conditionSrcVisible}`'
          default='CY_AUTANALOG_STT_AC_CONDITION_TR_AUTANALOG_IN0'
          editable='`${(conditionSrc == 1) &amp;&amp; conditionSrcVisible}`'>
        <Entry name='External Trigger in 0'        value='CY_AUTANALOG_STT_AC_CONDITION_TR_AUTANALOG_IN0'   visible='true'/>
        <Entry name='External Trigger in 1'        value='CY_AUTANALOG_STT_AC_CONDITION_TR_AUTANALOG_IN1'   visible='true'/>
        <Entry name='External Trigger in 2'        value='CY_AUTANALOG_STT_AC_CONDITION_TR_AUTANALOG_IN2'   visible='true'/>
        <Entry name='External Trigger in 3'        value='CY_AUTANALOG_STT_AC_CONDITION_TR_AUTANALOG_IN3'   visible='true'/>
        <Entry name='Device Active Mode'           value='CY_AUTANALOG_STT_AC_CONDITION_CHIP_ACTIVE'        visible='true'/>
        <Entry name='Device Deep-Sleep Mode'       value='CY_AUTANALOG_STT_AC_CONDITION_CHIP_DEEPSLEEP'     visible='true'/>
      </ParamChoice>

      <ParamChoice
          group='State Control'
          id='conditionSar'
          name='Condition'
          desc='Condition for SAR'
          visible='`${(conditionSrc == 2) &amp;&amp; conditionSrcVisible}`'
          default='CY_AUTANALOG_STT_AC_CONDITION_SAR_DONE'
          editable='`${(conditionSrc == 2) &amp;&amp; conditionSrcVisible}`'>
        <Entry name='SAR Done'                     value='CY_AUTANALOG_STT_AC_CONDITION_SAR_DONE'           visible='`${hasBlock("pass[0].sar[0]") &amp;&amp; isBlockUsed("pass[0].sar[0]")}`'/>
        <Entry name='SAR EOS'                      value='CY_AUTANALOG_STT_AC_CONDITION_SAR_EOS'            visible='`${hasBlock("pass[0].sar[0]") &amp;&amp; isBlockUsed("pass[0].sar[0]")}`'/>
        <Entry name='SAR Busy'                     value='CY_AUTANALOG_STT_AC_CONDITION_SAR_BUSY'           visible='`${hasBlock("pass[0].sar[0]") &amp;&amp; isBlockUsed("pass[0].sar[0]")}`'/>
        <Entry name='SAR FIR 0 Done'               value='CY_AUTANALOG_STT_AC_CONDITION_SAR_FIR0_DONE'      visible='`${hasBlock("pass[0].sar[0].firfilter[0]") &amp;&amp; isBlockUsed("pass[0].sar[0].firfilter[0]")}`'/>
        <Entry name='SAR FIR 1 Done'               value='CY_AUTANALOG_STT_AC_CONDITION_SAR_FIR1_DONE'      visible='`${hasBlock("pass[0].sar[0].firfilter[1]") &amp;&amp; isBlockUsed("pass[0].sar[0].firfilter[1]")}`'/>
        <Entry name='SAR FIFO Done'                value='CY_AUTANALOG_STT_AC_CONDITION_SAR_FIFO_DONE'      visible='`${hasBlock("pass[0].sar[0].fifo[0]") &amp;&amp; isBlockUsed("pass[0].sar[0].fifo[0]")}`'/>
        <Entry name='SAR FIR 0 FIFO Done'          value='CY_AUTANALOG_STT_AC_CONDITION_SAR_FIR0_FIFO_DONE' visible='`${hasBlock("pass[0].sar[0].fifo[0]") &amp;&amp; isBlockUsed("pass[0].sar[0].fifo[0]") &amp;&amp;
                                                                                                                        hasBlock("pass[0].sar[0].firfilter[0]") &amp;&amp; isBlockUsed("pass[0].sar[0].firfilter[0]")}`'/>
        <Entry name='SAR FIR 1 FIFO Done'          value='CY_AUTANALOG_STT_AC_CONDITION_SAR_FIR1_FIFO_DONE' visible='`${hasBlock("pass[0].sar[0].fifo[0]") &amp;&amp; isBlockUsed("pass[0].sar[0].fifo[0]") &amp;&amp;
                                                                                                                        hasBlock("pass[0].sar[0].firfilter[1]") &amp;&amp; isBlockUsed("pass[0].sar[0].firfilter[1]")}`'/>
        <Entry name='SAR Range Detect 0'           value='CY_AUTANALOG_STT_AC_CONDITION_SAR_RANGE0'         visible='`${hasBlock("pass[0].sar[0]") &amp;&amp; isBlockUsed("pass[0].sar[0]")}`'/>
        <Entry name='SAR Range Detect 1'           value='CY_AUTANALOG_STT_AC_CONDITION_SAR_RANGE1'         visible='`${hasBlock("pass[0].sar[0]") &amp;&amp; isBlockUsed("pass[0].sar[0]")}`'/>
        <Entry name='SAR Range Detect 2'           value='CY_AUTANALOG_STT_AC_CONDITION_SAR_RANGE2'         visible='`${hasBlock("pass[0].sar[0]") &amp;&amp; isBlockUsed("pass[0].sar[0]")}`'/>
        <Entry name='SAR Range Detect 3'           value='CY_AUTANALOG_STT_AC_CONDITION_SAR_RANGE3'         visible='`${hasBlock("pass[0].sar[0]") &amp;&amp; isBlockUsed("pass[0].sar[0]")}`'/>
        <Entry name='SAR FIFO 0 Level Detect'      value='CY_AUTANALOG_STT_AC_CONDITION_FIFO_LEVEL0'        visible='`${hasBlock("pass[0].sar[0].fifo[0]") &amp;&amp; isBlockUsed("pass[0].sar[0].fifo[0]")}`'/>
        <Entry name='SAR FIFO 1 Level Detect'      value='CY_AUTANALOG_STT_AC_CONDITION_FIFO_LEVEL1'        visible='`${hasBlock("pass[0].sar[0].fifo[0]") &amp;&amp; isBlockUsed("pass[0].sar[0].fifo[0]")}`'/>
