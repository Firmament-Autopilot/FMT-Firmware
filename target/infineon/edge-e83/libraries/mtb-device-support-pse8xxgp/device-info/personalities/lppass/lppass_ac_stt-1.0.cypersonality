        <Entry name='PTComp Comparator 1'          value='CY_AUTANALOG_STT_AC_CONDITION_PTCOMP_CMP1'        visible='`${hasBlock("pass[0].ptc[0]") &amp;&amp; isBlockUsed("pass[0].ptc[0]")}`'/>
