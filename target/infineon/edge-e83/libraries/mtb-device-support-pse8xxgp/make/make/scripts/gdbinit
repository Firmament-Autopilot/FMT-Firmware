# CM33 port
target remote:3333

# Open all memory
set mem inaccessible-by-default off

# Enable semihosting
monitor arm semihosting enable

# Load the executable
# load <LOCATION OF ELF FILE>

# Reset device
monitor reset init
flushregs
mon gdb_sync
stepi

# Set temporary breakpoint at main
tbreak main

# Print registers
monitor reg

# Continue program execution
continue