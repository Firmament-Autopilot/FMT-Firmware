{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "certificate": {
      "type": "object",
      "properties": {
        "type": {
          "type": "string",
          "enum": [
            "CONTENT_CERT"
          ]
        },
        "version": {
          "type": "number",
          "minimum": 1.0,
          "maximum": 1.0
        }
      },
      "required": [
        "type",
        "version"
      ]
    },
    "cert_keypair": {
      "type": "object",
      "properties": {
        "description": {
          "type": "string"
        },
        "value": {
          "type": "string"
        }
      },
      "required": [
        "value"
      ]
    },
    "cert_keypair_pwd": {
      "type": "object",
      "properties": {
        "description": {
          "type": "string"
        },
        "value": {
          "type": "string"
        }
      },
      "required": [
        "value"
      ]
    },
    "nv_counter": {
      "type": "object",
      "properties": {
        "description": {
          "type": "string"
        },
        "value": {
          "type": "integer",
          "minimum": 0,
          "maximum": 96
        }
      },
      "required": [
        "value"
      ]
    },
    "load_verify_scheme": {
      "type": "object",
      "properties": {
        "description": {
          "type": "string"
        },
        "value": {
          "type": "string",
          "enum": [
            "RAM_LOAD_VERIFY",
            "FLASH_VERIFY",
            "RAM_VERIFY",
            "RAM_LOAD"
          ]
        }
      },
      "required": [
        "value"
      ]
    },
    "encrypted": {
      "type": "object",
      "properties": {
        "description": {
          "type": "string"
        },
        "value": {
          "type": "boolean"
        }
      },
      "required": [
        "value"
      ]
    },
    "crypto_type": {
      "type": "object",
      "properties": {
        "description": {
          "type": "string"
        },
        "value": {
          "type": "string",
          "enum": [
            "PLAIN_IMAGE_HASH",
            "ENC_IMAGE_HASH"
          ]
        }
      },
      "required": [
        "value"
      ]
    },
    "image_table": {
      "type": "object",
      "properties": {
        "description": {
          "type": "string"
        },
        "value": {
          "type": "string"
        }
      },
      "required": [
        "value"
      ]
    }
  },
  "required": [
    "certificate",
    "cert_keypair",
    "nv_counter",
    "image_table"
  ]
}